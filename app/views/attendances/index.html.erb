<p id="notice"><%= notice %></p>

<h1>My Attendances</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Barcode</th>
      <th>In at</th>
      <th>Out at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attendances.each do |attendance| %>
      <tr>
        <td><%= attendance.username %></td>
        <td><%= attendance.barcode %></td>
        <td><%= attendance.in_at %></td>
        <td><%= attendance.out_at %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

